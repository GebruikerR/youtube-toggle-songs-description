{
  "name": "Hide songs in description",
  "description": "Hide Youtube Songs in Description. Currently only hiding, Toggling coming soon.",
  "version":"1.0",
  "manifest_version": 2,
  "content_scripts": [
    {
      "matches": ["https://*/*"],
      "run_at": "document_idle",
      "js": ["content.js"]
    }
  ],
  "permissions": [
    "https://*/*",
    "http://*/*",
    "tabs",
    "activeTab"
  ]
}
